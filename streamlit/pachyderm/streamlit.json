{
    "pipeline": {
      "name": "streamlit"
    },
    "description": "Streamlit to display an image.",
    "input": {
          "pfs": {
            "repo": "images",
            "glob": "/",
            "branch": "master"
          }
        },
    "service": {
        "external_port": 30888,
        "internal_port": 8501
    },
    "transform": {
        "cmd": [
          "streamlit","run","https://gist.githubusercontent.com/JimmyWhitaker/8fa48e46c0c8c598d5eb5810bb65da18/raw/2a18d553b98ceba44de631a4508950ad7bce72b5/image_gallary.py","--", "--image-path","/pfs/images/"
        ],
      "image": "jimmywhitaker/simple-streamlit:dev0.1"
    }
  }